<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/add_reservation_result"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  android:orientation="vertical"
  android:background="@color/white"
  >

     <RelativeLayout 
	     android:id="@+id/addResResultDialogIdLayout"
	     android:layout_width="fill_parent" 
	     android:layout_height="wrap_content"
	     android:padding="7dip"  
	     android:focusable="true" >
        
       <TextView
         android:id="@+id/addResResultDialogIdLabel"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:text="@string/add_reservation_dialog_id_label"
         android:minWidth="95dip"
         android:layout_alignParentLeft="true"
         android:layout_alignParentTop="true" />
       <TextView 
         android:id="@+id/addResResultDialogId"
         android:layout_width="fill_parent" 
         android:layout_height="wrap_content"
         android:textAppearance="@style/TextViewStyleParkingLotActivityBold"
         android:layout_toRightOf="@+id/addResResultDialogIdLabel"
         android:paddingLeft="5dip"
         android:layout_marginRight="15dip"
         android:singleLine="true" />
         
      </RelativeLayout>
      
      <RelativeLayout 
	     android:id="@+id/addResResultDialogStartTimeLayout"
	     android:layout_width="fill_parent" 
	     android:layout_height="wrap_content"
	     android:padding="7dip"  
	     android:focusable="true" >
        
       <TextView
         android:id="@+id/addResResultDialogStartTimeLabel"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:text="@string/add_reservation_dialog_start_time_label"
         android:minWidth="95dip"
         android:layout_alignParentLeft="true"
         android:layout_alignParentTop="true" />
       <TextView 
         android:id="@+id/addResResultDialogStartTime"
         android:layout_width="fill_parent" 
         android:layout_height="wrap_content"
         android:textAppearance="@style/TextViewStyleParkingLotActivityBold"
         android:layout_toRightOf="@+id/addResResultDialogStartTimeLabel"
         android:paddingLeft="5dip"
         android:layout_marginRight="15dip"
         android:singleLine="true" />
         
      </RelativeLayout>     
  		
  		<RelativeLayout 
	     android:id="@+id/addResResultDialogEndTimeLayout"
	     android:layout_width="fill_parent" 
	     android:layout_height="wrap_content"
	     android:padding="7dip"  
	     android:focusable="true" >
        
       <TextView
         android:id="@+id/addResResultDialogEndTimeLabel"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:text="@string/add_reservation_dialog_end_time_label"
         android:minWidth="95dip"
         android:layout_alignParentLeft="true"
         android:layout_alignParentTop="true" />
       <TextView 
         android:id="@+id/addResResultDialogEndTime"
         android:layout_width="fill_parent" 
         android:layout_height="wrap_content"
         android:textAppearance="@style/TextViewStyleParkingLotActivityBold"
         android:layout_toRightOf="@+id/addResResultDialogEndTimeLabel"
         android:paddingLeft="5dip"
         android:layout_marginRight="15dip"
         android:singleLine="true" />
         
      </RelativeLayout>
      
      <RelativeLayout 
	     android:id="@+id/addResResultDialogTotalTimeLayout"
	     android:layout_width="fill_parent" 
	     android:layout_height="wrap_content"
	     android:padding="7dip"  
	     android:focusable="true" >
        
       <TextView
         android:id="@+id/addResResultDialogTotalTimeLabel"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:text="@string/add_reservation_dialog_total_time_label"
         android:minWidth="95dip"
         android:layout_alignParentLeft="true"
         android:layout_alignParentTop="true" />
       <TextView 
         android:id="@+id/addResResultDialogTotalTime"
         android:layout_width="fill_parent" 
         android:layout_height="wrap_content"
         android:textAppearance="@style/TextViewStyleParkingLotActivityBold"
         android:layout_toRightOf="@+id/addResResultDialogTotalTimeLabel"
         android:paddingLeft="5dip"
         android:layout_marginRight="15dip"
         android:singleLine="true" />
         
      </RelativeLayout>
      
     <RelativeLayout 
	     android:id="@+id/addResResultDialogPriceLayout"
	     android:layout_width="fill_parent" 
	     android:layout_height="wrap_content"
	     android:padding="7dip"  
	     android:focusable="true" >
        
       <TextView
         android:id="@+id/addResResultDialogPriceLabel"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:text="@string/add_reservation_dialog_price_label"
         android:minWidth="95dip"
         android:layout_alignParentLeft="true"
         android:layout_alignParentTop="true" />
       <TextView 
         android:id="@+id/addResResultDialogPrice"
         android:layout_width="fill_parent" 
         android:layout_height="wrap_content"
         android:textAppearance="@style/TextViewStyleParkingLotActivityBold"
         android:layout_toRightOf="@+id/addResResultDialogPriceLabel" 
         android:paddingLeft="5dip"
         android:layout_marginRight="15dip"
         android:singleLine="true" />
         
      </RelativeLayout>
      
      <RelativeLayout 
	     android:id="@+id/addResResultDialogCostLayout"
	     android:layout_width="fill_parent" 
	     android:layout_height="wrap_content"
	     android:padding="7dip"  
	     android:focusable="true" >
        
       <TextView
         android:id="@+id/addResResultDialogCostLabel"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:text="@string/add_reservation_dialog_cost_label"
         android:minWidth="95dip"
         android:layout_alignParentLeft="true"
         android:layout_alignParentTop="true" />
       <TextView 
         android:id="@+id/addResResultDialogCost"
         android:layout_width="fill_parent" 
         android:layout_height="wrap_content"
         android:textAppearance="@style/TextViewStyleParkingLotActivityBold"
         android:layout_toRightOf="@+id/addResResultDialogCostLabel" 
         android:paddingLeft="5dip"
         android:layout_marginRight="15dip"
         android:singleLine="true" />
         
      </RelativeLayout>      

</LinearLayout>